# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_API_TIMEOUT=10000

# App Configuration
EXPO_PUBLIC_APP_NAME=Owles
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_ENVIRONMENT=development

# Authentication
EXPO_PUBLIC_JWT_SECRET=your-jwt-secret-key
EXPO_PUBLIC_TOKEN_EXPIRY=24h

# Database
EXPO_PUBLIC_DB_URL=sqlite://./database.sqlite

# Push Notifications
EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED=true

# Feature Flags
EXPO_PUBLIC_DARK_MODE_ENABLED=true
EXPO_PUBLIC_OFFLINE_MODE_ENABLED=true
EXPO_PUBLIC_ANALYTICS_ENABLED=false

# Development
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
